{
  "receiverType": "GPUBuffer",
  "requirements": ["GPUBuffer"],
  "methods": [
    {
      "name": "mapAsync",
      "returnType": "none",
      "async": true,
      "properties": [
        {
          "mode": {
            "type": "bitwiseFlag",
            "enum": [
              "GPUMapMode.READ",
              "GPUMapMode.WRITE"
            ],
            "conditions": {
              "bufferCompatibleMapping": true
            }
          }
        }
      ],
      "conditions": {
        "withAttributes": {
          "GPUBuffer.mapped": ["false"],
          "GPUBuffer.usage": ["GPUBufferUsage.MAP_READ", "GPUBufferUsage.MAP_WRITE"]
        }
      },
      "setAttributes": {
        "this.mapped": ["true"],
        "this.mappedAtCreation": ["false"]
      }
    },
    {
      "name": "getMappedRange",
      "returnType": "ArrayBuffer",
      "setUnavailable": {
        "this": ["GPUBuffer.getMappedRange"]
      },
      "conditions": {
        "availableForGettingMappedRange": true,
        "bufferCompatibleMapping": true,
        "withAttributes": {
          "GPUBuffer.usage": ["GPUBufferUsage.MAP_READ", "GPUBufferUsage.MAP_WRITE"]
        }
      }
    },
    {
      "name": "unmap",
      "returnType": "none",
      "conditions": {
        "withAttributes": {
          "GPUBuffer.mapped": ["true"]
        }
      },
      "setAttributes": {
        "this.mapped": ["false"]
      }
    }
  ],
  "attributes": [
    {
      "name": "label",
      "returnType": "string"
    }
  ]
}