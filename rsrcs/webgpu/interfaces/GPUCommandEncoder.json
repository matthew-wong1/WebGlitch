{
  "receiverType": "GPUCommandEncoder",
  "requirements": ["GPUCommandEncoder"],
  "methods": [
    {
      "name": "beginRenderPass",
      "paramType": "object",
      "returnType": "GPURenderPassEncoder",
      "properties": [
        {
          "colorAttachments": {
            "type": "colorAttachments",
            "array": true
          },
          "label": {
            "type": "string",
            "optional": true
          },
          "maxDrawCount": {
            "type": "uint",
            "optional": true
          },
          "occlusionQuerySet": {
            "type": "GPUQuerySet",
            "conditions": {
              "same": ["GPUDevice"]
            },
            "optional": true
          }
        }
      ],
      "setUnavailable": {
        "this": ["GPUCommandEncoder.beginRenderPass", "GPUCommandEncoder.beginComputePass"]
      }
    },
    {
      "name": "beginComputePass",
      "paramType": "object",
      "returnType": "GPUComputePassEncoder",
      "properties": [
        {
          "label": {
            "type": "string",
            "optional": true
          }
        }
      ],
      "setUnavailable": {
        "this": ["GPUCommandEncoder.beginRenderPass", "GPUCommandEncoder.beginComputePass"]
      }
    },
    {
      "name": "finish",
      "returnType": "GPUCommandBuffer",
      "conditions": {
        "commandEncodingFinished": true
      },
      "setUnavailable": {
        "this": ["all"],
        "children": {
          "GPURenderPassEncoder": ["all"],
          "GPUComputePassEncoder": ["all"]
        }
      }
    }
  ],
  "attributes": [
    {
      "name": "label",
      "returnType": "string"
    }
  ]
}