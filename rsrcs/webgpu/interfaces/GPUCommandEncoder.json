{
  "receiverType": "GPUCommandEncoder",
  "requirements": ["GPUCommandEncoder"],
  "methods": [
    {
      "name": "beginRenderPass",
      "paramType": "object",
      "returnType": "GPURenderPassEncoder",
      "properties": [
        {
          "colorAttachments": {
            "type": "colorAttachments",
            "array": true
          },
          "label": {
            "type": "string",
            "optional": true
          },
          "maxDrawCount": {
            "type": "uint",
            "optional": true
          },
          "occlusionQuerySet": {
            "type": "GPUQuerySet",
            "conditions": {
              "same": ["GPUDevice"]
            },
            "optional": true
          }
        }
      ],
      "setUnavailable": {
        "this": ["GPUCommandEncoder.beginRenderPass", "GPUCommandEncoder.beginComputePass"]
      }
    },
    {
      "name": "beginComputePass",
      "paramType": "object",
      "returnType": "GPUComputePassEncoder",
      "properties": [
        {
          "label": {
            "type": "string",
            "optional": true
          }
        }
      ],
      "setUnavailable": {
        "this": ["GPUCommandEncoder.beginRenderPass", "GPUCommandEncoder.beginComputePass"]
      }
    },
    {
      "name": "finish",
      "returnType": "GPUCommandBuffer",
      "conditions": {
        "commandEncodingFinished": true
      },
      "setUnavailable": {
        "this": ["all"],
        "children": {
          "GPURenderPassEncoder": ["all"],
          "GPUComputePassEncoder": ["all"]
        }
      }
    },
    {
      "name": "copyBufferToBuffer",
      "returnType": "none",
      "properties": [
        {
          "source": {
            "type": "GPUBuffer",
            "conditions": {
              "same": ["GPUDevice"],
              "withAttributes": {
                "GPUBuffer.usage": ["GPUBufferUsage.COPY_SRC"],
                "GPUBuffer.mappedAtCreation": ["false"]
              }
            }
          },
          "sourceOffset": {
            "type": "uint",
            "conditions": {
              "min": 0,
              "max": "this.source.size",
              "divisible": 4
            }
          },
          "destination": {
            "type": "GPUBuffer",
            "conditions": {
              "same": ["GPUDevice"],
              "differentTo": "source",
              "withAttributes": {
                "GPUBuffer.usage": ["GPUBufferUsage.COPY_DST"],
                "GPUBuffer.mappedAtCreation": ["false"]
              }
            }
          },
          "destinationOffset": {
            "type": "uint",
            "conditions": {
              "min": 0,
              "max": "this.destination.size",
              "divisible": 4
            }
          },
          "size": {
            "type": "uint",
            "conditions": {
              "fitsInBuffer": true
            }
          }
        }
      ]
    }
  ],
  "attributes": [
    {
      "name": "label",
      "returnType": "string"
    }
  ]
}