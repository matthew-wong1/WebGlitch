{
  "receiverType": "GPUDevice",
  "requirements": ["GPUDevice"],
  "methods": [
    {
      "name": "createBuffer",
      "paramType": "object",
      "returnType": "GPUBuffer",
      "properties": [
        {
          "label": {
            "type": "string",
            "optional": true
          },
          "mappedAtCreation": {
            "type": "boolean",
            "optional": true
          },
          "size": {
            "type": "uint",
            "conditions": {
              "max": 268435456,
              "divisible": 4
            }
          },
          "usage": {
            "type": "bitwiseFlag",
            "enum": [
              "GPUBufferUsage.COPY_SRC",
              "GPUBufferUsage.COPY_DST",
              "GPUBufferUsage.INDEX",
              "GPUBufferUsage.INDIRECT",
              "GPUBufferUsage.MAP_READ",
              "GPUBufferUsage.MAP_WRITE",
              "GPUBufferUsage.QUERY_RESOLVE",
              "GPUBufferUsage.STORAGE",
              "GPUBufferUsage.UNIFORM",
              "GPUBufferUsage.VERTEX"
            ],
            "mutex": [
              {
                "name": "GPUBufferUsage.MAP_WRITE",
                "allowed": ["GPUBufferUsage.COPY_DST"]
              },
              {
                "name": "GPUBufferUsage.MAP_READ",
                "allowed": ["GPUBufferUsage.COPY_DST"]
              }
            ]
          }
        }
      ]
    },
    {
      "name": "createCommandEncoder",
      "paramType": "object",
      "returnType": "GPUCommandEncoder",
      "properties": [
        {
          "label": {
            "type": "string",
            "optional": true
          }
        }
      ]
    },
    {
      "name": "createQuerySet",
      "paramType": "object",
      "returnType": "GPUQuerySet",
      "properties": [
        {
          "count": {
            "type": "uint",
            "conditions": {
              "max": 4096
            }
          },
          "label": {
            "type": "string",
            "optional": true
          },
          "type": {
            "type": "string",
            "enum": [
              "occlusion"
            ]
          }
        }
      ]
    },
    {
      "name": "createTexture",
      "paramType": "object",
      "returnType": "GPUTexture",
      "properties": [
        {
          "dimension": {
            "type": "string",
            "enum": [
              "1d",
              "2d",
              "3d"
            ],
            "optional": true
          },
          "format": {
            "type": "gpuTextureFormat",
            "enum": true
          },
          "label": {
            "type": "string",
            "optional": true
          },
          "mipLevelCount": {
            "type": "uint",
            "optional": true
          },
          "sampleCount": {
            "type": "int",
            "enum": [
              1,
              4
            ],
            "optional": true
          },
          "size": {
            "type": "textureSize"
          },
          "usage": {
            "type": "gpuTextureUsage",
            "enum": true
          },
          "viewFormats": {
            "type": "gpuTextureFormat",
            "optional": true,
            "enum": true,
            "array": true,
            "conditions": {
              "textureCompatible": "format"
            }
          }
        }
      ]
    }
  ],
  "attributes": [
    {
      "name": "features",
      "returnType": "GPUSupportedFeatures"
    },
    {
      "name": "label",
      "returnType": "string"
    },
    {
      "name": "limits",
      "returnType": "GPUSupportedLimits"
    },
    {
      "name": "queue",
      "returnType": "GPUQueue"
    }
  ]
}