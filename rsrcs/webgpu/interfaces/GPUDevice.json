{
  "receiverType": "GPUDevice",
  "requirements": ["GPUDevice"],
  "methods": [
    {
      "name": "createBuffer",
      "paramType": "object",
      "returnType": "GPUBuffer",
      "properties": [
        {
          "label": {
            "type": "string",
            "optional": true
          },
          "mappedAtCreation": {
            "type": "boolean",
            "optional": true
          },
          "size": {
            "type": "uint",
            "conditions": {
              "max": 268435456,
              "divisible": 4
            }
          },
          "usage": {
            "type": "bitwiseFlag",
            "enum": [
              "GPUBufferUsage.COPY_SRC",
              "GPUBufferUsage.COPY_DST",
              "GPUBufferUsage.INDEX",
              "GPUBufferUsage.INDIRECT",
              "GPUBufferUsage.MAP_READ",
              "GPUBufferUsage.MAP_WRITE",
              "GPUBufferUsage.QUERY_RESOLVE",
              "GPUBufferUsage.STORAGE",
              "GPUBufferUsage.UNIFORM",
              "GPUBufferUsage.VERTEX"
            ],
            "conditions": {
              "uniformStorageCompatible": true
            },
            "mutex": [
              {
                "name": "GPUBufferUsage.MAP_WRITE",
                "allowed": ["GPUBufferUsage.COPY_SRC"]
              },
              {
                "name": "GPUBufferUsage.MAP_READ",
                "allowed": ["GPUBufferUsage.COPY_DST"]
              }
            ]
          }
        }
      ],
      "setAttributes": {
        "returnVariable.mapped": ["this.mappedAtCreation"]
      }
    },
    {
      "name": "createCommandEncoder",
      "paramType": "object",
      "returnType": "GPUCommandEncoder",
      "properties": [
        {
          "label": {
            "type": "string",
            "optional": true
          }
        }
      ]
    },
    {
      "name": "createQuerySet",
      "paramType": "object",
      "returnType": "GPUQuerySet",
      "properties": [
        {
          "count": {
            "type": "uint",
            "conditions": {
              "max": 4096
            }
          },
          "label": {
            "type": "string",
            "optional": true
          },
          "type": {
            "type": "string",
            "enum": [
              "occlusion"
            ]
          }
        }
      ]
    },
    {
      "name": "createTexture",
      "paramType": "object",
      "returnType": "GPUTexture",
      "properties": [
        {
          "dimension": {
            "type": "string",
            "enum": [
              "1d",
              "2d",
              "3d"
            ],
            "optional": true
          },
          "sampleCount": {
            "type": "int",
            "enum": true,
            "conditions": {
              "constraints": true,
              "enum": {
                "dimension": {
                  "1d": [1],
                  "2d": [1, 4],
                  "3d": [1]
                }
              }
            },
            "optional": true
          },
          "format": {
            "type": "gpuTextureFormat",
            "enum": true,
            "inFile": true,
            "conditions": {
              "textureDimensionAndSampleCompatible": true
            }
          },
          "label": {
            "type": "string",
            "optional": true
          },
          "size": {
            "type": "textureSize"
          },
          "mipLevelCount": {
            "type": "uint",
            "optional": true,
            "conditions": {
              "constraints": true,
              "max": {
                "customValidation": "mipLevelCount"
              },
              "min": 1
            }
          },
          "usage": {
            "type": "gpuTextureUsage",
            "enum": true,
            "inFile": true,
            "conditions": {
              "textureFormatCompatible": true,
              "textureUsageCompatible": "dimension",
              "multiSamplingCompatible": true
            }
          },
          "viewFormats": {
            "type": "gpuTextureFormat",
            "optional": true,
            "enum": true,
            "inFile": true,
            "array": true,
            "conditions": {
              "textureCompatible": "format"
            }
          }
        }
      ]
    },
    {
      "name": "createShaderModule",
      "paramType": "object",
      "returnType": "GPUShaderModule",
      "properties": [
        {
          "label": {
            "type": "string",
            "optional": true
          },
          "code": {
            "type": "shader"
          }
        }
      ]
    },
    {
      "name": "createRenderPipeline",
      "paramType": "object",
      "returnType": "GPURenderPipeline",
      "properties": [
        {
          "fragment": {
            "type": "fragment",
            "optional": true
          },
          "label": {
            "type": "string",
            "optional": true
          },
          "layout": {
            "type": "gpuLayout"
          },
          "multisample": {
            "type": "multisample",
            "optional": true
          },
          "primitive": {
            "type": "primitive",
            "optional": true
          },
          "vertex": {
            "type": "vertex"
          }
        }
      ]
    },
    {
      "name": "createRenderPipelineAsync",
      "paramType": "object",
      "async": true,
      "returnType": "GPURenderPipeline",
      "properties": [
        {
          "fragment": {
            "type": "fragment",
            "optional": true
          },
          "label": {
            "type": "string",
            "optional": true
          },
          "layout": {
            "type": "gpuLayout"
          },
          "multisample": {
            "type": "multisample",
            "optional": true
          },
          "primitive": {
            "type": "primitive",
            "optional": true
          },
          "vertex": {
            "type": "vertex"
          }
        }
      ]
    },
    {
      "name": "createComputePipeline",
      "paramType": "object",
      "returnType": "GPUComputePipeline",
      "properties": [
        {
          "compute": {
            "type": "compute"
          },
          "label": {
            "type": "string",
            "optional": true
          },
          "layout": {
            "type": "gpuLayout"
          }
        }
      ]
    },
    {
      "name": "createComputePipelineAsync",
      "paramType": "object",
      "async": true,
      "returnType": "GPUComputePipeline",
      "properties": [
        {
          "compute": {
            "type": "compute"
          },
          "label": {
            "type": "string",
            "optional": true
          },
          "layout": {
            "type": "gpuLayout"
          }
        }
      ]
    },
    {
      "name": "createBindGroup",
      "returnType": "GPUBindGroup",
      "paramType": "object",
      "properties": [
        {
          "label": {
            "type": "string",
            "optional": true
          },
          "layout": {
            "type": "bindGroupLayout",
            "conditions": {
              "computeShaderCompatible": true,
              "same": [
                "GPUDevice"
              ]
            }
          },
          "entries": {
            "type": "bindGroupEntry",
            "array": true,
            "arrayType": "bindGroupEntry",
            "conditions": {
              "computeShaderCompatible": true
            }
          }
        }
      ]
    },
    {
      "name": "createSampler",
      "returnType": "GPUSampler"
    }
  ],
  "attributes": [
    {
      "name": "features",
      "returnType": "GPUSupportedFeatures"
    },
    {
      "name": "label",
      "returnType": "string"
    },
    {
      "name": "limits",
      "returnType": "GPUSupportedLimits"
    },
    {
      "name": "queue",
      "returnType": "GPUQueue"
    }
  ]
}