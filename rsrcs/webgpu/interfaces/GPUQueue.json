{
  "receiverType": "GPUQueue",
  "requirements": ["GPUQueue"],
  "methods": [
    {
      "name": "writeBuffer",
      "returnType": "none",
      "properties": [
        {
          "buffer": {
            "type": "GPUBuffer",
            "conditions": {
              "same": ["GPUDevice"],
              "withAttributes": {
                "GPUBuffer.usage": ["GPUBufferUsage.COPY_DST"],
                "GPUBuffer.mappedAtCreation": ["false"]
              }
            }
          },
          "bufferOffset": {
            "type": "uint",
            "conditions": {
              "min": 0,
              "max": "this.buffer.size",
              "divisible": 4
            }
          },
          "data": {
            "type": "typedArray",
            "conditions": {
              "fitsInBuffer": true
            }
          }
        }
      ]
    },
    {
      "name": "submit",
      "returnType": "none",
      "properties": [
        {
          "commandBuffers": {
            "type": "GPUCommandBuffer",
            "conditions": {
              "same": [
                "GPUDevice"
              ]
            },
            "array": true
          }
        }
      ],
      "deletes": "commandBuffers"
    }
  ],
  "attributes": [
    {
      "name": "label",
      "returnType": "string"
    }
  ]
}