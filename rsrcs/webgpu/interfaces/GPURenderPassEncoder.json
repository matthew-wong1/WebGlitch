{
  "receiverType": "GPURenderPassEncoder",
  "requirements": ["GPURenderPassEncoder"],
  "methods": [
    {
      "name": "setPipeline",
      "returnType": "none",
      "properties": [
        {
          "pipeline": {
            "type": "GPURenderPipeline"
          }
        }
      ]
    },
    {
      "name": "setVertexBuffer",
      "returnType": "none",
      "properties": [
        {
          "slot": {
            "type": "bufferSlot"
          },
          "buffer": {
            "type": "GPUBuffer"
          }
        }
      ]
    },
    {
      "name": "draw",
      "returnType": "none",
      "properties": [
        {
          "vertexCount": {
            "type": "uint",
            "conditions": {
              "min": 1,
              "max": 10
            }
          }
        }
      ]
    },
    {
      "name": "finish",
      "returnType": "GPUCommandBuffer",
      "conditions": {
        "commandEncodingFinished": true
      }
    }
  ],
  "attributes": [
    {
      "name": "label",
      "returnType": "string"
    }
  ]
}