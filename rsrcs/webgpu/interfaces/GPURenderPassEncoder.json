{
  "receiverType": "GPURenderPassEncoder",
  "requirements": ["GPURenderPassEncoder"],
  "methods": [
    {
      "name": "setPipeline",
      "returnType": "none",
      "properties": [
        {
          "pipeline": {
            "type": "GPURenderPipeline",
            "conditions": {
              "renderPassCompatible": true
            }
          }
        }
      ]
    },
    {
      "name": "setVertexBuffer",
      "returnType": "none",
      "properties": [
        {
          "slot": {
            "type": "bufferSlot"
          },
          "buffer": {
            "type": "GPUBuffer",
            "conditions": {
              "withAttributes": {
                "GPUBuffer.usage": ["GPUBufferUsage.VERTEX"],
                "GPUBuffer.mappedAtCreation": ["false"]
              }
            }
          }
        }
      ]
    },
    {
      "name": "draw",
      "returnType": "none",
      "properties": [
        {
          "vertexCount": {
            "type": "uint",
            "conditions": {
              "min": 1,
              "max": 10
            }
          }
        }
      ],
      "conditions": {
        "renderPipelineAvailable": true
      }
    },
    {
      "name": "end",
      "returnType": "none",
      "conditions": {
        "renderPassEncodingFinished": true
      },
      "setUnavailable": {
        "this": ["all"]
      },
      "setAvailable": {
        "parents": {
          "GPUCommandEncoder": ["GPUCommandEncoder.beginRenderPass", "GPUCommandEncoder.beginComputePass"]
        }
      }
    }

  ],
  "attributes": [
    {
      "name": "label",
      "returnType": "string"
    }
  ]
}