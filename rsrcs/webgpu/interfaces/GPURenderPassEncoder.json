{
  "receiverType": "GPURenderPassEncoder",
  "requirements": ["GPURenderPassEncoder"],
  "methods": [
    {
      "name": "insertDebugMarker",
      "returnType": "none",
      "properties": [
        {
          "markerLabel": {
            "type": "markerLabel"
          }
        }
      ]
    },
    {
      "name": "setPipeline",
      "returnType": "none",
      "properties": [
        {
          "pipeline": {
            "type": "GPURenderPipeline",
            "conditions": {
              "same": ["GPUDevice"],
              "renderPassCompatible": true
            }
          }
        }
      ]
    },
    {
      "name": "setVertexBuffer",
      "returnType": "none",
      "properties": [
        {
          "slot": {
            "type": "bufferSlot"
          },
          "buffer": {
            "type": "GPUBuffer",
            "conditions": {
              "same": ["GPUDevice"],
              "withAttributes": {
                "GPUBuffer.usage": ["GPUBufferUsage.VERTEX"],
                "GPUBuffer.mappedAtCreation": ["false"]
              },
              "trackedLifetime": true
            }
          }
        }
      ]
    },
    {
      "name": "draw",
      "returnType": "none",
      "properties": [
        {
          "vertexCount": {
            "type": "uint",
            "conditions": {
              "min": 1,
              "max": 10
            }
          }
        }
      ],
      "conditions": {
        "renderPipelineAvailable": true
      }
    },
    {
      "name": "setBlendConstant",
      "returnType": "none",
      "properties": [
        {
          "color": {
            "type": "clearValue",
            "optional": true
          }
        }
      ]
    },
    {
      "name": "setStencilReference",
      "returnType": "none",
      "properties": [
        {
          "reference": {
            "type": "uint",
            "conditions": {
              "min": 1,
              "max": 10
            }
          }
        }
      ],
      "conditions": {
        "renderPipelineAvailable": true
      }
    },
    {
      "name": "setIndexBuffer",
      "returnType": "none",
      "properties": [
        {
          "buffer": {
            "type": "GPUBuffer",
            "conditions": {
              "same": ["GPUDevice"],
              "withAttributes": {
                "GPUBuffer.usage": ["GPUBufferUsage.INDEX"],
                "GPUBuffer.mappedAtCreation": ["false"]
              },
              "trackedLifetime": true
            }
          }
        },
        {
          "indexFormat": {
            "type": "string",
            "enum": [
              "uint16",
              "uint32"
            ]
          }
        }
      ]
    },
    {
      "name": "pushDebugGroup",
      "returnType": "none",
      "properties": [
        {
          "markerLabel": {
            "type": "markerLabel"
          }
        }
      ]
    },
    {
      "name": "popDebugGroup",
      "returnType": "none",
      "properties": [
        {
          "markerLabel": {
            "type": "markerLabel"
          }
        }
      ],
      "conditions": {
        "debugGroupAvailable": true
      }
    },
    {
      "name": "end",
      "returnType": "none",
      "conditions": {
        "noActiveDebugGroups": true,
        "renderPassEncodingFinished": true
      },
      "setUnavailable": {
        "this": ["all"]
      },
      "setAvailable": {
        "parents": {
          "GPUCommandEncoder": ["GPUCommandEncoder.beginRenderPass", "GPUCommandEncoder.beginComputePass", "GPUCommandEncoder.copyBufferToBuffer", "GPUCommandEncoder.insertDebugMarker", "GPUCommandEncoder.clearBuffer"]
        }
      }
    }

  ],
  "attributes": [
    {
      "name": "label",
      "returnType": "string"
    }
  ]
}