{
  "clearValue": [
    {
      "r": {
        "type": "rgba"
      },
      "g": {
        "type": "rgba"
      },
      "b": {
        "type": "rgba"
      },
      "a": {
        "type": "rgba"
      }
    }
  ],

  "colorAttachment": [
    {
      "clearValue": {
        "type": "clearValue",
        "optional": true
      },
      "loadOp": {
        "type": "string",
        "enum": [
          "clear",
          "load"
        ]
      },
      "storeOp": {
        "type": "string",
        "enum": [
          "discard",
          "store"
        ]
      },
      "resolveTarget": {
        "type": "GPUTextureView",
        "optional": true
      },
      "view": {
        "type": "GPUTextureView"
      }
    }
  ],

  "depthStencilAttachment": [
    {
      "depthClearValue": {
        "type": "double",
        "conditions": {
          "min": 0,
          "max": 1
        },
        "optional": true
      },
      "depthReadOnly": {
        "type": "boolean",
        "optional": true
      },
      "depthStoreOp": {
        "type": "string",
        "optional": true,
        "enum": [
          "discard",
          "store"
        ]
      },
      "stencilClearValue": {
        "type": "uint",
        "optional": true
      },
      "stencilLoadOp": {
        "type": "string",
        "optional": true,
        "enum": [
          "clear",
          "load"
        ]
      },
      "stencilReadOnly": {
        "type": "boolean",
        "optional": true
      },
      "stencilStoreOp": {
        "type": "string",
        "optional": true,
        "enum": [
          "discard",
          "store"
        ]
      },
      "view": {
        "type": "GPUTextureView"
      }
    }
  ],

  "textureSize": [
    {
      "width": {
        "type": "uint",
        "conditions": {
          "min": 1,
          "max": {
            "constraints": {
              "dimension": {
                "1d": 8192,
                "2d": 8192,
                "3d": 2048
              }
            }
          }
        }
      },
      "height": {
        "type": "uint",
        "conditions": {
          "min": 1,
          "max": {
            "constraints": {
              "dimension": {
                "1d": 1,
                "2d": 8192,
                "3d": 2048
              }
            }
          }
        },
        "optional": true
      },
      "depthOrArrayLayers": {
        "type": "uint",
        "conditions": {
          "min": 1,
          "max": {
            "constraints": {
              "dimension": {
                "1d": 1,
                "2d": 256,
                "3d": 2048
              },
              "sampleCount": {
                "4": 1
              }
            }
          }
        }
      }
    }
  ],
  "depthStencil": [
    {
      "depthBias": {
        "type": "ufloat",
        "optional": true
      },
      "depthBiasClamp": {
        "type": "ufloat",
        "optional": true
      },
      "depthBiasSlopeScale": {
        "type": "ufloat",
        "optional": true
      },
      "depthCompare": {
        "type": "depthFragmentTest",
        "enum": true,
        "inFile": true
      },
      "depthWriteEnabled": {
        "type": "boolean"
      },
      "format": {
        "type": "gpuTextureFormat",
        "conditions": {
          "depthOrStencil": true
        }
      },
      "stencilBack": {
        "type": "stencilBack"
      },
      "stencilFront": {
        "type": "stencilBack"
      },
      "stencilReadMask": {
        "type": "bitmask"
      },
      "stencilWriteMask": {
        "type": "bitmask"
      }
    }
  ],
  "fragment": [
    {
      "entryPoint": {
        "type": "shaderEntryPoint"
      },
      "module": {
        "type": "GPUShaderModule",
        "conditions": {
          "withAttributes": {
            "label": "fragment"
          }
        }
      },
      "targets": {
        "type": "targets",
        "array": true
      }
    }
  ],
  "targets": [
    {
      "blend": {
        "type": "blend",
        "optional": true
      },
      "format": {
        "type": "gpuTextureFormat"
      },
      "writeMask": {
        "type": "bitwiseFlags",
        "enum": [
          "GPUFlagsConstant.RED",
          "GPUFlagsConstant.GREEN",
          "GPUFlagsConstant.BLUE",
          "GPUFlagsConstant.ALPHA",
          "GPUFlagsConstant.ALL"
        ],
        "optional": true
      }
    }
  ],
  "blend": [
    {
      "alpha": {
        "type": "blendFactor"
      },
      "color": {
        "type": "blendFactor"
      }
    }
  ],
  "blendFactor": [
    {
      "dstFactor": {
        "type": "blendFactorOperation",
        "enum": true,
        "inFile": true,
        "optional": true
      },
      "operation": {
        "type": "string",
        "optional": true,
        "enum": [
          "add",
          "max",
          "min",
          "reverse-subtract",
          "subtract"
        ]
      },
      "srcFactor": {
        "type": "blendFactorOperation",
        "enum": true,
        "inFile": true,
        "optional": true
      }
    }
  ],
  "multisample": [
    {
      "alphaToCoverageEnabled": {
        "optional": true,
        "type": "boolean"
      },
      "count": {
        "type": "uint",
        "conditions": {
          "attachmentTextureCompatible": true
        },
        "optional": true
      },
      "mask": {
        "type": "bitmask",
        "optional": true
      }
    }
  ],
  "primitive": [
    {

    }
  ],
  "vertex": [
    {

    }
  ],
  "stencilBack": [
    {
      "compare": {
        "type": "depthFragmentTest",
        "enum": true,
        "inFile": true,
        "optional": true
      },
      "depthFailOp": {
        "type": "depthFailOp",
        "enum": true,
        "inFile": true,
        "optional": true
      },
      "failOp": {
        "type": "depthFailOp",
        "enum": true,
        "inFile": true,
        "optional": true
      },
      "passOp": {
        "type": "depthFailOp",
        "enum": true,
        "inFile": true,
        "optional": true
      }
    }
  ]
}